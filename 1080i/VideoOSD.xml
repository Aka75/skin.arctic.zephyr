<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="2901">
    <defaultcontrol always="true">15</defaultcontrol>
    <allowoverlay>yes</allowoverlay>

    <controls>
        <control type="group">
            <animation type="WindowOpen">
                <effect type="fade" start="0" end="100" time="150"/>
                <effect type="slide" start="0,-64" end="0" time="150"/>
            </animation>
            <animation type="WindowClose">
                <effect type="fade" end="0" start="100" time="150"/>
                <effect type="slide" end="0,-64" start="0" time="150"/>
            </animation>
            <animation type="Visible">
                <effect type="fade" start="0" end="100" time="150"/>
                <effect type="slide" start="0,-64" end="0" time="150"/>
            </animation>
            <animation type="Hidden">
                <effect type="fade" end="0" start="100" time="150"/>
                <effect type="slide" end="0,-64" start="0" time="150"/>
            </animation>
            <control type="image">
                <width>100%</width>
                <height>74</height>
                <texture colordiffuse="OSDBack">common/white.png</texture>
            </control>
            <!-- Sub Menu -->
            <control type="group">
                <visible allowhiddenfocus="true">ControlGroup(9300).HasFocus() | Control.HasFocus(3)</visible>

                <include>Animation.SlideUp</include>
                <include>Animation.FadeOut</include>
                <posy>72</posy>
                <centerleft>560</centerleft>
                <width>316</width>
                <height>152</height>
                <control type="image">
                    <description>background image</description>
                    <width>100%</width>
                    <height>100%</height>
                    <texture border="16" colordiffuse="Light1">common/rounded-shadow8.png</texture>
                </control>
                <control type="grouplist" id="9300">
                    <include>Animation.FadeIn.Delay</include>
                    <include>Animation.FadeOut</include>
                    <top>16</top>
                    <bottom>16</bottom>
                    <left>8</left>
                    <right>8</right>
                    <onup>3</onup>
                    <ondown>3</ondown>
                    <onleft>3</onleft>
                    <onright>3</onright>
                    <itemgap>0</itemgap>
                    <orientation>vertical</orientation>
                    <scrolltime>200</scrolltime>
                    <control type="button" id="9301" description="Download">
                        <width>100%</width>
                        <height>60</height>
                        <textcolor>Dark2</textcolor>
                        <include>DefContextButton</include>
                        <label>33003</label>
                        <onclick>ActivateWindow(SubtitleSearch)</onclick>
                    </control>
                    <control type="togglebutton" id="9302" description="Enable Subs">
                        <width>100%</width>
                        <height>60</height>
                        <textcolor>Dark2</textcolor>
                        <include>DefContextButton</include>
                        <label>305</label>
                        <altlabel>13106</altlabel>
                        <onclick>Action(ShowSubtitles)</onclick>
                        <usealttexture>!VideoPlayer.SubtitlesEnabled</usealttexture>
                    </control>
                </control>
            
            </control>
            
            <control type="grouplist" id="100">
                <top>13</top>
                <height>48</height>
                <itemgap>16</itemgap>
                <orientation>horizontal</orientation>
                <align>left</align>
                <onleft>6</onleft>
                <onright condition="!Player.CanRecord | !VideoPlayer.Content(LiveTV)">12</onright>
                <onright condition="Player.CanRecord + VideoPlayer.Content(LiveTV)">9003</onright>
                <onup>noop</onup>
                <ondown>noop</ondown>
                <usecontrolcoords>true</usecontrolcoords>
        
                

                <control type="button" id="1">
                    <description>Audio</description>
                    <label>292</label>
                    <onclick>ActivateWindow(124)</onclick>
                    <include>DefOSDButtons</include>
                </control>

                <control type="button" id="2">
                    <description>Video Settings</description>
                    <label>291</label>
                    <onclick>ActivateWindow(123)</onclick>
                    <include>DefOSDButtons</include>
                </control>
                
                <control type="button" id="3">
                    <ondown>9301</ondown>
                    <description>Subs</description>
                    <label>$LOCALIZE[287]</label>
                    <onclick>Action(ShowSubtitles)</onclick>
                    <include>DefOSDButtons</include>
                </control>
            </control>
            <control type="grouplist" id="101">
            
                <top>13</top>
                <width>100%</width>
                <height>48</height>
                <itemgap>16</itemgap>
                <orientation>horizontal</orientation>
                <align>center</align>
                <onleft>3</onleft>
                <onright>4</onright>
                <ondown>noop</ondown>
           
                <control type="togglebutton" id="9003">
                    <description>Record</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight" flipx="true">osd/record.png</texturefocus>
                    <texturenofocus colordiffuse="White70" flipx="true">osd/record.png</texturenofocus>
                    <usealttexture>Player.Recording</usealttexture>
                    <alttexturefocus colordiffuse="Highlight" flipx="true">osd/record-on.png</alttexturefocus>
                    <alttexturenofocus colordiffuse="White70" flipx="true">osd/record-on.png</alttexturenofocus>
                    <onclick>PlayerControl(Record)</onclick>
                    <enable>Player.CanRecord</enable>
                    <visible>VideoPlayer.Content(LiveTV)</visible>
                    <animation effect="fade" start="100" end="33" time="100" condition="!Player.CanRecord">Conditional</animation>
                </control>
                
                <control type="button" id="12">
                    <description>Skip Back</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight" flipx="true">osd/next.png</texturefocus>
                    <texturenofocus colordiffuse="White70" flipx="true">osd/next.png</texturenofocus>
                    <onclick>PlayerControl(Previous)</onclick>
                </control>

                <control type="button" id="13">
                    <description>Rewind</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight" flipx="true">osd/ff.png</texturefocus>
                    <texturenofocus colordiffuse="White70" flipx="true">osd/ff.png</texturenofocus>
                    <onclick>PlayerControl(Rewind)</onclick>
                    <enable>Player.SeekEnabled</enable>
                    <animation effect="fade" start="100" end="33" time="100" condition="!Player.SeekEnabled">Conditional</animation>
                </control>

                <control type="togglebutton" id="14">
                    <description>Play/Pause</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <altlabel></altlabel>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight">osd/pause.png</texturefocus>
                    <texturenofocus colordiffuse="White70">osd/pause.png</texturenofocus>
                    <usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
                    <alttexturefocus colordiffuse="Highlight">osd/play.png</alttexturefocus>
                    <alttexturenofocus colordiffuse="White70">osd/play.png</alttexturenofocus>
                    <onclick>PlayerControl(Play)</onclick>
                    <enable>Player.PauseEnabled</enable>
                    <animation effect="fade" start="100" end="33" time="100" condition="!Player.PauseEnabled">Conditional</animation>
                </control>

                <control type="button" id="15">
                    <description>Stop</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight">osd/stop.png</texturefocus>
                    <texturenofocus colordiffuse="White70">osd/stop.png</texturenofocus>
                    <onclick>PlayerControl(Stop)</onclick>
                </control>

                <control type="button" id="16">
                    <description>Fast Forward</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight">osd/ff.png</texturefocus>
                    <texturenofocus colordiffuse="White70">osd/ff.png</texturenofocus>
                    <onclick>PlayerControl(Forward)</onclick>
                    <enable>Player.SeekEnabled</enable>
                    <animation effect="fade" start="100" end="33" time="100" condition="!Player.SeekEnabled">Conditional</animation>
                </control>

                <control type="button" id="17">
                    <description>Next</description>
                    <width>48</width>
                    <height>48</height>
                    <label></label>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight">osd/next.png</texturefocus>
                    <texturenofocus colordiffuse="White70">osd/next.png</texturenofocus>
                    <onclick>PlayerControl(Next)</onclick>
                </control>
                
                <control type="button" id="9002">
                    <width>48</width>
                    <height>48</height>
                    <font>noop</font>
                    <include>DefOSDUpDown</include>
                    <texturefocus colordiffuse="Highlight">osd/eject.png</texturefocus>
                    <texturenofocus colordiffuse="White70">osd/eject.png</texturenofocus>
                    <onclick>EjectTray()</onclick>
                    <visible>System.HasMediadvd</visible>
                </control>
                <include condition="!Window.IsVisible(fullscreenvideo) + !Window.IsVisible(visualisation)">VideoOSDFullscreen</include>
            
            </control>
            
            <control type="grouplist" id="102">
                <top>13</top>
                <right>0</right>
                <height>48</height>
                <itemgap>16</itemgap>
                <orientation>horizontal</orientation>
                <align>right</align>
                <onleft>17</onleft>
                <onright>1</onright>
                <onup>noop</onup>
                <ondown>noop</ondown>
                <usecontrolcoords>true</usecontrolcoords>
        
                <control type="button" id="4">
                    <description>DVD Menu</description>
                    <label>$LOCALIZE[31116]</label>
                    <include>DefOSDButtons</include>
                    <onclick>PlayerControl(ShowVideoMenu)</onclick>
                    <enable>VideoPlayer.HasMenu</enable>
                    <disabledcolor>White12</disabledcolor>
                </control>

                <control type="button" id="5">
                    <description>3D Mode</description>
                    <label>31117</label>
                    <onclick>StereoMode</onclick>
                    <include>DefOSDButtons</include>
                    <enable>VideoPlayer.IsStereoscopic</enable>
                    <disabledcolor>White12</disabledcolor>
                </control>

                <control type="button" id="6">
                    <description>BookMarks</description>
                    <include>DefOSDButtons</include>
                    <label>298</label>
                    <onclick>ActivateWindow(125)</onclick>
                </control>
            </control>
        </control>
    
    </controls>

</window>
